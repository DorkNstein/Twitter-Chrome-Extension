{
  "manifest_version": 2,

  "name": "Tweets",
  "description": "This extension shows Tweets",
  "version": "1.0",

  "background":{
    "page":"background.html",
    "persistence": false
    },

  "permissions": [
     "tabs",
    "notifications",
    "https://*/*"
  ],
  "content_security_policy": "img-src 'self' http://*/*; object-src 'self'; script-src 'self'"
}
